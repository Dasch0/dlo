  <MissileTypes>
    <MissileTemplate>
      <AssociatedTemplateName>SGM-100 Anti-Hybrid</AssociatedTemplateName>
      <Designation>SGM-100</Designation>
      <Nickname>Anti-Hybrid</Nickname>
      <Description>DIRECT - ACT(&lt;color=#3BC81E&gt;RADAR&lt;/color&gt;) - HE FRAG</Description>
